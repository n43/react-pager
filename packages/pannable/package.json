{
  "name": "react-pannable",
  "version": "0.6.0",
  "description": "Simulate pan gesture and scroll view for touch devices with React",
  "keywords": [
    "react",
    "scroll",
    "pan",
    "drag",
    "grid",
    "list",
    "carousel",
    "slide",
    "swiper",
    "page",
    "bounce",
    "table",
    "collection",
    "virtualized",
    "infinite-scroll"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n43/react-pannable",
  "repository": {
    "type": "git",
    "url": "https://github.com/n43/react-pannable.git"
  },
  "bugs": {
    "url": "https://github.com/n43/react-pannable/issues"
  },
  "author": "Zhu DeMing <mingenesis@qq.com>",
  "contributors": [
    "Chen SiHui <502672047@qq.com>"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-pannable.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "NODE_ENV=development rollup -c -o dist/react-pannable.js",
    "build:umd:min": "NODE_ENV=production rollup -c -o dist/react-pannable.min.js",
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min",
    "prebuild": "yarn clean",
    "clean": "rimraf lib dist es coverage",
    "pretest": "yarn build",
    "test": "yarn lint",
    "prepare": "yarn test",
    "lint": "eslint src",
    "cover": "yarn lint",
    "prepublishOnly": "cd ../demo && yarn deploy"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "dependencies": {
    "element-resize-detector": "1.1.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.5.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
