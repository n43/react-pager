{
  "name": "react-pannable",
  "version": "5.0.0",
  "description": "Flexible and Customizable Layouts for Scrolling Content with React",
  "keywords": [
    "react",
    "hooks",
    "scroll",
    "pan",
    "drag",
    "grid",
    "list",
    "carousel",
    "slide",
    "swiper",
    "page",
    "bounce",
    "table",
    "collection",
    "virtualized",
    "infinite-scroll"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n43/react-pannable",
  "repository": {
    "type": "git",
    "url": "https://github.com/n43/react-pannable.git",
    "directory": "packages/pannable"
  },
  "bugs": {
    "url": "https://github.com/n43/react-pannable/issues"
  },
  "author": "Zhu DeMing <mingenesis@qq.com>",
  "contributors": [
    "Chen SiHui <502672047@qq.com>"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "cjs",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib es cjs types dist coverage",
    "lint": "eslint --ext ts,tsx src",
    "coverage": "yarn lint",
    "tsc": "tsc",
    "build:es": "babel lib -d es",
    "build:cjs": "BABEL_ENV_MODULES=cjs babel lib -d cjs",
    "build:umd": "rollup -c",
    "build": "yarn lint && yarn tsc && yarn build:es && yarn build:cjs && yarn build:umd",
    "test": "jest -v",
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "cd ../demo && yarn deploy"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "dependencies": {
    "@types/element-resize-detector": "^1.1.2",
    "element-resize-detector": "1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.19.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "react": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3"
  }
