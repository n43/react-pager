{
  "name": "react-pannable",
  "version": "6.0.8",
  "description": "Flexible and Customizable Layouts for Scrolling Content with React",
  "keywords": [
    "react",
    "hooks",
    "scroll",
    "pan",
    "drag",
    "grid",
    "list",
    "carousel",
    "slide",
    "swiper",
    "page",
    "bounce",
    "table",
    "collection",
    "virtualized",
    "infinite-scroll"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n43/react-pannable",
  "repository": {
    "type": "git",
    "url": "https://github.com/n43/react-pannable.git",
    "directory": "packages/pannable"
  },
  "bugs": {
    "url": "https://github.com/n43/react-pannable/issues"
  },
  "author": "Zhu DeMing <mingenesis@qq.com>",
  "contributors": [
    "Chen SiHui <502672047@qq.com>"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "cjs",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib es cjs types dist coverage",
    "lint": "eslint --ext ts,tsx src",
    "coverage": "yarn lint",
    "tsc": "tsc",
    "build:es": "babel lib -d es",
    "build:cjs": "BABEL_ENV_MODULES=cjs babel lib -d cjs",
    "build:umd": "rollup -c",
    "build": "yarn lint && yarn tsc && yarn build:es && yarn build:cjs && yarn build:umd",
    "test": "jest -v",
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "cd ../demo && yarn deploy"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
    "react-dom": "^16.8.4 || ^17.0.0"
  },
  "dependencies": {
    "@types/element-resize-detector": "^1.1.3",
    "element-resize-detector": "1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/node": "*",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  }
