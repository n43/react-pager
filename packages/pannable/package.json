{
  "name": "react-pannable",
  "version": "0.2.1",
  "description": "",
  "keywords": [
    "react",
    "react-component"
  ],
  "homepage": "https://github.com/n43/react-pannable",
  "bugs": "https://github.com/n43/react-pannable/issues",
  "license": "MIT",
  "author": "Zhu DeMing <mingenesis@qq.com>",
  "contributors": [
    "Chen SiHui <502672047@qq.com>"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "NODE_ENV=development rollup -c -o dist/react-pannable.js",
    "build:umd:min": "NODE_ENV=production rollup -c -o dist/react-pannable.min.js",
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min",
    "clean": "rimraf lib dist es coverage",
    "prepare": "yarn lint && yarn clean && yarn build",
    "lint": "eslint src",
    "test": "jest",
    "coverage": "codecov"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "dependencies": {
    "element-resize-detector": "1.1.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
