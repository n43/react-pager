{
  "name": "react-pannable",
  "version": "2.0.0",
  "description": "Flexible and Customizable Layouts for Scrolling Content with React",
  "keywords": [
    "react",
    "hooks",
    "scroll",
    "pan",
    "drag",
    "grid",
    "list",
    "carousel",
    "slide",
    "swiper",
    "page",
    "bounce",
    "table",
    "collection",
    "virtualized",
    "infinite-scroll"
  ],
  "license": "MIT",
  "homepage": "https://github.com/n43/react-pannable",
  "repository": {
    "type": "git",
    "url": "https://github.com/n43/react-pannable.git",
    "directory": "packages/pannable"
  },
  "bugs": {
    "url": "https://github.com/n43/react-pannable/issues"
  },
  "author": "Zhu DeMing <mingenesis@qq.com>",
  "contributors": [
    "Chen SiHui <502672047@qq.com>"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-pannable.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "NODE_ENV=development rollup -c -o dist/react-pannable.js",
    "build:umd:min": "NODE_ENV=production rollup -c -o dist/react-pannable.min.js",
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min",
    "prebuild": "yarn clean",
    "clean": "rimraf lib dist es coverage",
    "pretest": "yarn build",
    "test": "yarn lint",
    "prepare": "yarn test",
    "lint": "eslint src",
    "cover": "yarn lint",
    "prepublishOnly": "cd ../demo && yarn deploy"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "dependencies": {
    "element-resize-detector": "1.1.15"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@typescript-eslint/eslint-plugin": "1.x",
    "@typescript-eslint/parser": "1.x",
    "babel-eslint": "10.x",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.4",
    "eslint": "5.x",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "jest": "^24.8.0",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^2.6.3",
    "rollup": "1.16.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.4",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-uglify": "6.0.2"
  }
}
